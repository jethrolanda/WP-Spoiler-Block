{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wp-spoiler/wp-spoiler-block",
  "version": "0.1.0",
  "title": "Wp Spoiler Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "attributes": {
    "editMode": {
      "type": "boolean",
      "default": true
    },
    "content": {
      "type": "string",
      "default": ""
    },
    "value": {
      "type": "number",
      "default": 0
    }
  },
  "textdomain": "wp-spoiler-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}